month = nov,
number = {21},
pages = {7479--85},
title = {{A protocol to assess the enzymatic release of dissolved organic phosphorus species in waters under environmentally relevant conditions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18044529},
volume = {41},

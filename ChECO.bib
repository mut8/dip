file = {::},
issn = {0003-2697},
journal = {Analytical biochemistry},
keywords = {Amino Acids,Amino Acids: analysis,Animals,Carbon Isotopes,Chromatography, Gas,Chromatography, Gas: methods,Esters,Esters: analysis,Male,Mass Spectrometry,Mass Spectrometry: methods,Rats,Rats, Wistar},
month = feb,
number = {2},
pages = {156--64},
title = {{13C gas chromatography-combustion isotope ratio mass spectrometry analysis of N-pivaloyl amino acid esters of tissue and plasma samples.}},

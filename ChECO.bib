journal = {Trends in ecology \& evolution (Personal edition)},
keywords = {Biochemistry,Biochemistry: methods,Biodiversity,Genes,Genes: physiology,Geologic Sediments,Geologic Sediments: microbiology,Geology,Geology: methods,Humic Substances,Humic Substances: analysis,Humic Substances: microbiology,Phylogeny,Plants,Plants: chemistry,Plants: metabolism,Plants: microbiology,Soil Microbiology},
month = jun,

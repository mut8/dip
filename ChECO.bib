month = jun,
number = {6},
pages = {1520--30},
title = {{Aboveground overyielding in grassland mixtures is associated with reduced biomass partitioning to belowground organs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19569367},
volume = {90},
year = {2009}
}

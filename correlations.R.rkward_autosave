h2.pro<-harvest[harvest==2 | harvest==6]==2
h3.pro<-harvest[harvest==2 | harvest==6]==6

tmp<-
cbind(processes, controls)

colnames(tmp)
tmp
processes
controls

cond<-h2.pro
write.csv(corr.ab(tmp[cond,T], tmp[cond,T], pomit=T),"all_cor_h2.csv")
cond<-h3.pro
write.csv(corr.ab(tmp[cond,T], tmp[cond,T], pomit=T),"all_cor_h3.csv")


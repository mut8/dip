@article{Hodgkin1952,
abstract = {This article concludes a series of papers concerned with the flow of electric current through the surface membrane of a giant nerve fibre (Hodgkin, Huxley \& Katz, 1952; Hodgkin \& Huxley, 1952 a-c). Its general object is to discuss the results of the preceding papers (Part I), to put them into mathematical form (Part II) and to show that they will account for conduction and excitation in quantitative terms (Part III).},
annote = {This paper is the last in a series of classic experiments by Hodgkin and Huxley to determine the ionic basis of action potential generation and conduction in nerve fibers, for which they were awarded the Nobel Prize in 1963. It provides a synthesis of the preceding papers through a quantitative description of the various conductances that contribute to the action potential. This description was notable not only for its accuracy (the Hodgkin and Huxley equations are still used in most computer simulations of neurons to this day) but because it was able to precicely predict structural properties of ion channels that were not previously known. The paper thus serves as a model for what all physiological investigations should aspire to do. },
author = {Hodgkin, A L and Huxley, A F},

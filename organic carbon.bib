journal = {Chemosphere},
keywords = {Carbon,Carbon: analysis,China,Environmental Monitoring,Geologic Sediments,Geologic Sediments: analysis,Halogenated Diphenyl Ethers,Halogenated Diphenyl Ethers: analysis,Rivers,Soil Pollutants,Soil Pollutants: analysis,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis},
number = {2},
pages = {226--31},
pmid = {19395064},
title = {{PBDEs in sediments of the Beijiang River, China: levels, distribution, and influence of total organic carbon.}},

@article{Zhang2009,
abstract = {This study aims to investigate the state of the riverine organic carbon in the Luodingjiang River under human impacts, such as reforestation, construction of reservoirs and in-stream damming. Seasonal and spatial characteristics of total suspended sediment (TSS), dissolved organic carbon (DOC) and particulate organic carbon (POC), as well as C/N ratios and the stable carbon isotopic signatures of POC (delta(13)C(POC)) were examined based on a one-year study (2005) in the basin-wide scale. More frequent sampling was conducted in the outlet of the river basin at Guanliang hydrological station. DOC and POC concentrations showed flush effects with increasing water discharge and sediment load in the basin-wide scale. Atomic C/N ratio of POC had a positive relationship with TSS in the outlet of the basin, indicating the reduced aquatic sources and enhanced terrestrial sources during the high flood season. However, the similar relationship was not observed in the basin-wide scale mainly due to the spatial distributions of soil organic carbon and TSS. delta(13)C(POC) showed obvious seasonal variations with enriched values in the period with high TSS concentration, reflecting the increased contribution from C(4) plants with enhanced soil erosion. The specific flux of the total organic carbon (2.30 t km(-)(2) year(-1)) was smaller than the global average level. The ratio of DOC to POC was 1.17, which is higher than most rivers under Asian monsoon climate regime. The organic carbon flux was estimated to decline with decreasing sediment load as a result of reforestation, reservoir construction and in-stream damming, which demonstrates the impacts of human disturbances on the global carbon cycle.},
author = {Zhang, Shurong and Lu, X X and Sun, Huiguo and Han, Jingtai and Higgitt, David Laurence},
doi = {10.1016/j.scitotenv.2008.09.022},
file = {::},
issn = {0048-9697},
journal = {The Science of the total environment},
keywords = {Altitude,Carbon,Carbon: analysis,China,Environmental Monitoring,Floods,Geography,Geologic Sediments,Geologic Sediments: analysis,Geologic Sediments: chemistry,Humans,Nitrogen,Nitrogen: analysis,Organic Chemicals,Organic Chemicals: analysis,Rivers,Rivers: chemistry,Seasons,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis},
number = {2},
pages = {815--25},
pmid = {19004473},
title = {{Geochemical characteristics and fluxes of organic carbon in a human-disturbed mountainous river (the Luodingjiang River) of the Zhujiang (Pearl River), China.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19004473},
volume = {407},
year = {2009}
}
@article{Mattsson2007,
abstract = {The role of organic and minerogenic acidity in controlling pH levels in Finnish rivers was studied by measuring total organic carbon (TOC) and non-marine sulphate (*SO(4)) concentrations in river water of the main river basins and their sub-basins. The basins are located along a latitudinal gradient (60 degrees N to 69 degrees N) and cover 297,322 km(2), 88\% of the total area of Finland. The basins are predominately covered by coniferous forests and peatlands, and are located in areas with low sulphate deposition (80-430 mg S m(-2) a(-1)). The proportion of the basin covered by forests on mineral soils ranges from 29 to 64\% and the proportion of peatlands from 3 to 60\%. The percentage of peatland is highest in northern Finland (28-60\%), whereas the proportion of forests on mineral soils increases towards the south. The majority of the agricultural land is located in southern and western Finland. *SO(4) concentrations were positively correlated with the proportion of agricultural land in the basin. Moreover, the percentage of peatlands had a positive correlation with the concentrations of TOC and organic anion. High peatland proportion and high TOC and organic anion concentrations decreased pH values in the river water, whereas no correlation between *SO(4) concentrations and pH was observed. The average organic anion concentration exceeded the average *SO(4) concentration in river water in 17 basins out of the 86 studied basins. The organic anion-dominated areas were situated in northern basins, reflecting the high extent of peatlands in these areas. *SO(4) dominated in southern Finland and in western coastal areas, where the extent of agricultural land, acid sulphate soils and the deposition of sulphate is highest.},
author = {Mattsson, Tuija and Kortelainen, Pirkko and Lepist\"{o}, Ahti and R\"{a}ike, Antti},
doi = {10.1016/j.scitotenv.2007.05.013},
file = {::},
issn = {0048-9697},
journal = {The Science of the total environment},
keywords = {Agriculture,Carbon,Carbon: analysis,Environmental Monitoring,Finland,Hydrogen-Ion Concentration,Metals,Metals: analysis,Rivers,Rivers: chemistry,Sulfates,Sulfates: analysis,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis},
number = {1-3},
pages = {183--92},
pmid = {17572478},
title = {{Organic and minerogenic acidity in Finnish rivers in relation to land use and deposition.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17572478},
volume = {383},
year = {2007}
}

cond <- alldata$Harvest=="I"

lst<-data.frame(alldata$C_lit[cond], alldata$C_lit[cond], alldata$DOC[cond],alldata$N_lit[cond], alldata$P_lit[cond],alldata$C.N_lit[cond], alldata$C.P_lit[cond],alldata$N.P_lit[cond], alldata$K[cond], alldata$K[cond], alldata$CA[cond], alldata$MG[cond], alldata$FE[cond], alldata$MN[cond], alldata$ZN[cond])

sig<-1

for (i in 1:13)
{
  aov<-anova(lm(lst[,i]~type[cond]))
